local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local WindowKey = OrionLib:MakeWindow({Name = "[ðŸŽƒHALLOWEENðŸŽƒ] msedge - Key System", HidePremium = false, IntroEnabled = true, IntroText = "[ðŸŽƒHALLOWEENðŸŽƒ] msedge - Key System", SaveConfig = true, ConfigFolder = "msedgeKeySystem"})

--Values

_G.Key = "msedgeContainer"
_G.KeyInput = "string"

--Functions

function IncorrectKeyNotification()
    OrionLib:MakeNotification({
        Name = "Incorrect Key!",
        Content = "You have entered a Incorrect Key!",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

function Clickboard()
    OrionLib:MakeNotification({
        Name = "Information",
        Content = "Check your Clipboard!",
        Image = "rbxassetid://4483345998",
        Time = 8
    })
end



function MakeScriptHub()
    OrionLib:Destroy()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/coolperson-sudo/SlapBattles/refs/heads/main/msedgeHalloween"))()
end

--KeySystem 2/2

local KeyTab = WindowKey:MakeTab({
    Name = "Get Key",
    Icon = "rbxassetid://6023426915",
    PremiumOnly = false
})

KeyTab:AddButton({
    Name = "Get Key",
    Callback = function()
            setclipboard("https://linkvertise.com/1245606/msedge-key-system?o=sharing") -- Your Link To get Key
            toclipboard("https://linkvertise.com/1245606/msedge-key-system?o=sharing") -- Your Link To get Key
            Clickboard()
            end
})


KeyTab:AddTextbox({
    Name = "Enter Key",
    Default = "",
    TextDisappear = true,
    Callback = function(Value)
        _G.KeyInput = Value
    end
})

KeyTab:AddButton({
    Name = "Check Key",
    Callback = function()
              if _G.KeyInput == _G.Key then
            MakeScriptHub()
            else
                IncorrectKeyNotification()
            end
      end
})
